import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		while(T-- > 0)
		{
		    int X = sc.nextInt();
		    int Y = sc.nextInt();
		    
		    int a = 10;
		    int b = 90;
		    
		    System.out.println(X*a + Y*b);
		}
	}
}
