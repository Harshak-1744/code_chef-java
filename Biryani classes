import java.util.Scanner;

class Codechef 
{
    public static void main(String[] args) throws java.lang.Exception   
    {
        Scanner sc = new Scanner(System.in);
        int T = sc.nextInt();
        for (int i = 0; i < T; i++)
        {
            int a = sc.nextInt();
            int b = sc.nextInt();
            
            int result = a * b;
            
            System.out.println(result);
        }
    }
}
