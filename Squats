import java.util.*;

class Codechef 
{
    public static void main(String[] args) throws java.lang.Exception 
    {
        Scanner sc = new Scanner(System.in);
        int T = sc.nextInt();
        int S = 15;
        for (int i = 0; i < T; i++) 
        {
            int X = sc.nextInt();
            int totalSquats = X * S;
            System.out.println(totalSquats);
        }
    }
}
