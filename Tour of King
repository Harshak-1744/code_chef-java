import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int T = sc.nextInt();
		for(int i = 1;i<=T;i++)
		{
		    int N = sc.nextInt();
		    int M = sc.nextInt();
		    
		    int N_res = N * 5;
		    int M_res = M * 7;
		    
		    System.out.println(N_res + M_res);
		}
	}
}
